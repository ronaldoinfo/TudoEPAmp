.content-width {
    width: 80%;
}
.content {
    margin: 0 auto;
    padding-top: 50px;
    padding-bottom: 50px;
}

/* Header */
/* header,  */
.hdr {
    /* background: var(--bs-roxoazulado); */
    position: sticky;
    top: 0;
    z-index: 999;
}
#sidebar {
    /*width: 360px;*/
max-width: 100%;
width: calc(10% + 40vh);
}
.sidebarView {
display: grid;
grid-template-rows: auto 1fr auto;
height: 100vh;
position: fixed;
width: 100%;
overflow: hidden;
}
.tdepScrollView {
overflow-y: scroll;
overflow-x: hidden;
height: 100%;
-webkit-overflow-scrolling: touch;
box-sizing: border-box;
}
.tdep-menuShareButtons {
display: flex;
align-items: center;
width: 100%;
height: 50px;
box-sizing: border-box;
padding: 0 18px;
}
.sidebar-menu {
    background: var(--bs-white);
    padding: 0px;
    margin: 0px;
    font-weight: 400;
}
.amp-custom-sidebar-header {
    line-height: 3.5rem;
    min-height: 3.5rem;
    padding: 0 1rem;
    // color: var(--bs-white);
    // height: 90px;
}
#sidebar-close {
    cursor: pointer;
    margin-left: auto;
    // font-size: 1.2rem;
}
.sidebar-nav-item {
    padding: 1rem 20px;
    margin: 0px;
    list-style: none;
    text-transform: capitalize;
    letter-spacing: .6px;
    // line-height: 2.0625;
}
.sidebar-nav-link {
    cursor: pointer;
    // text-decoration: none;
    // color: var(--bs-secondary);
}
.sidebar-nav-link:hover {
    text-decoration: underline;
    // color: var(--bs-primary);
}
.menu {
    display: flex;
    justify-content: space-between;
    margin: 0 auto;
}
.sidebar-menu-btn,
#sidebar-menu-btn {
    background: 0 0;
    border: 0;
    /* color: var(--bs-white); */
    color: var(--bs-roxoazulado);
    font-weight: 800;
    font-size: 16px;
    /*display: none;*/
}
